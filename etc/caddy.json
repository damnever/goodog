{
  "apps": {
    "http": {
      "servers": {
        "goodog": {
          "automatic_https": {
            "disable": false,
            "disable_redirects": true
          },
          "experimental_http3": true,
          "listen": ["<DOMAIN>"],
          "read_timeout": "30s",
          "write_timeout": "10s",
          "routes": [
            {
              "handle": [
                {
                  "handler": "goodog",
                  "upstream_tcp": "<TCP-SERVER>",
                  "upstream_udp": "<UDP-SERVER>",
                  "connect_timeout": "10s",
                  "read_timeout": "1m",
                  "write_timeout": "10s",
                  "path": "/",
                  "users": [{"name": "knock", "password": "knock"}]
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  }
}
